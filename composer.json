{
    "name": "wedevelopnl/silverstripe-icon-manager",
    "description": "Silverstripe module to upload and manage custom icons to use in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "silverstripe/asset-admin": "^2.0",
        "silverstripe/cms": "^5",
        "wedevelopnl/silverstripe-svg-image": "dev-feature/ss-5"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\IconManager\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    }
}
