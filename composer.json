{
    "name": "wedevelopnl/silverstripe-icon-manager",
    "description": "Silverstripe module to upload and manager custom icons to use in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "silverstripe/asset-admin": "^1@stable",
        "silverstripe/cms": "^4@stable",
        "wedevelopnl/silverstripe-svg-image": "^1@stable"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\IconManager\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    }
}
