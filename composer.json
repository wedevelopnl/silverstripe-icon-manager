{
    "name": "thewebmen/silverstripe-icon-manager",
    "description": "Silverstripe module to upload and manager custom icons to use in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Webmen",
        "email": "development@webmen.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "silverstripe/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\IconManager\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    }
}
